package org.length;

public class Data {
	
public static void main(String[] args) {
	
	String s0 = "Green Technology";
	String s1 = "SeleniumAutomationtool";
	String s2 = "velmurugan";
	String s3 = "j a v a p r o g r a m";
	String s4 = "9095484678";		
	int length0 = s0.length();
	int length1 = s1.length();
	int length2 = s2.length();
	int length3 = s3.length();
	int length4 = s4.length();
	System.out.println("total number of character = "+length0);
	System.out.println("total number of character = "+length1);
	System.out.println("total number of character = "+length2);
	System.out.println("total number of character = "+length3);
	System.out.println("total number of character = "+length4);
	
}
}
