package org.colour;//same package

import org.paint.Client;

//defalut same package....using obj create
public class Employee extends Client {// public
	
	private void empid() {

		System.out.println("Employee id is: 6758904555");
	}
	private void empname() {
System.out.println("Employee name is : virat");
	}
	public static void main(String[] args) {
		Employee e = new Employee();
		e.empid();
		e.empname();
		
		Studend s = new Studend();
		s.studentid();
		s.studentname();
		
	}}
