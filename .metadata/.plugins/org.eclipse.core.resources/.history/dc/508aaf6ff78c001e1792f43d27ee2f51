package org.testing;

public class Employee {   // class
	
	
	
	private  void employeeId() {  //method
		
		
		System.out.println("employee id is 1234");
		
		

	}
	
	
	
	private void empName() {
		
		
		System.out.println("employe name is Anushya");
	}

//program execution
	
	public static void main(String[] args) {
		
		Employee emp = new Employee();  // object creation
		
		
                  emp.employeeId();  // to call the method
				emp.empName();
				
		
	}
		
		
		
	}
	
	

