package org.length;

public class Index {
	public static void main(String[] args) {
		String s1 = "Green Technology";
		String s2 = "SeleniumAutomationtool";
		String s3 = "Velmurugan";
		String s4 = "j a v a p r o g r a m";
		String s5 = "9095484678";
		
		int indexOf1 = s1.indexOf("o");
		int indexOf2 = s2.indexOf("o");
		int indexOf3= s3.indexOf("n");
		int indexOf4 = s4.indexOf("last index");
		int indexOf5 = s5.indexOf(8);
		
		
		System.out.println("the last index of 0  = "+indexOf1);
		System.out.println("the index of o  = "+indexOf2);
		System.out.println("the index of n = "+indexOf3);
		System.out.println("the last index of empty space ="+indexOf4);
		System.out.println("the index of 8= "+indexOf5);
} 
}