package org.file;

import java.io.File;

public class Data {
	public static void main(String[] args) {
		//existing path.......C:\\java
		File f = new File("C:\\java\\Batch1\\c\\html\\testing");
		boolean mkdirs = f.mkdirs();//to create a multiple folder folder
		System.out.println(mkdirs);
		
	}

}
