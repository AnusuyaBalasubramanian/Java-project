package org.length;

public class Index {
	public static void main(String[] args) {
		String s1 = "Green Technology";
		String s2 = "SeleniumAutomationtool";
		String s3 = "Velmurugan";
		String s4 = "j a v a p r o g r a m ";
		String s5 = "9095484678";
		
		int index1 = s1.length();
		int index2= s2.length();
		int index3 = s3.length();
		int index4 = s4.length();
		int index5 = s5.length();
		
		System.out.println("the last index of 0  = "+(s1.length()-1));
		System.out.println("the index of o  = "+(s2.length()-11));
		System.out.println("the index of n = "+(s3.length()-1));
} 
}